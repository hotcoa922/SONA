<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".writediary.HashTagFragment">


    <TextView
        android:id="@+id/announcement_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:textStyle="bold"
        android:textSize="30sp"
        android:textColor="@color/button_light_intext_color"
        android:text="해시태그 선택하기(최대 8개)"
        app:layout_constraintBottom_toTopOf="@+id/profile_guideline1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/profile_guideline1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.066" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@color/button_light_intext_color"
        android:text="#기본 해쉬태그"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/profile_guideline1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/profile_guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.20" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:text="#사용자 지정 해쉬태그"
        android:textColor="@color/button_light_intext_color"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/profile_guideline2" />

    <Button
        android:id="@+id/df_hstag_bt1"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="#느긋한"
        app:layout_constraintBottom_toTopOf="@+id/df_hstag_bt5"
        app:layout_constraintEnd_toStartOf="@+id/df_hstag_bt2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/df_hstag_bt2"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="#행복한"
        app:layout_constraintBottom_toBottomOf="@+id/df_hstag_bt1"
        app:layout_constraintEnd_toStartOf="@+id/df_hstag_bt3"
        app:layout_constraintStart_toEndOf="@+id/df_hstag_bt1"
        app:layout_constraintTop_toTopOf="@+id/df_hstag_bt1" />

    <Button
        android:id="@+id/df_hstag_bt3"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="#신나는"
        app:layout_constraintBottom_toBottomOf="@+id/df_hstag_bt2"
        app:layout_constraintEnd_toStartOf="@+id/df_hstag_bt4"
        app:layout_constraintStart_toEndOf="@+id/df_hstag_bt2"
        app:layout_constraintTop_toTopOf="@+id/df_hstag_bt2" />

    <Button
        android:id="@+id/df_hstag_bt4"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="#흥분한"
        app:layout_constraintBottom_toBottomOf="@+id/df_hstag_bt3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/df_hstag_bt3"
        app:layout_constraintTop_toTopOf="@+id/df_hstag_bt3" />

    <Button
        android:id="@+id/df_hstag_bt5"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="#긴장한"
        app:layout_constraintBottom_toTopOf="@+id/profile_guideline2"
        app:layout_constraintEnd_toStartOf="@+id/df_hstag_bt6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/df_hstag_bt1" />

    <Button
        android:id="@+id/df_hstag_bt6"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="#괴로운"
        app:layout_constraintBottom_toBottomOf="@+id/df_hstag_bt5"
        app:layout_constraintEnd_toStartOf="@+id/df_hstag_bt7"
        app:layout_constraintStart_toEndOf="@+id/df_hstag_bt5"
        app:layout_constraintTop_toTopOf="@+id/df_hstag_bt5" />

    <Button
        android:id="@+id/df_hstag_bt7"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="#우울한"
        app:layout_constraintBottom_toBottomOf="@+id/df_hstag_bt6"
        app:layout_constraintEnd_toStartOf="@+id/df_hstag_bt8"
        app:layout_constraintStart_toEndOf="@+id/df_hstag_bt6"
        app:layout_constraintTop_toTopOf="@+id/df_hstag_bt6" />

    <Button
        android:id="@+id/df_hstag_bt8"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="#무기력"
        app:layout_constraintBottom_toBottomOf="@+id/df_hstag_bt7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/df_hstag_bt7"
        app:layout_constraintTop_toTopOf="@+id/df_hstag_bt7" />

    <Button
        android:id="@+id/addHashtagNameButton"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginTop="23dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:text="추가하기"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/profile_guideline3"
        app:layout_constraintEnd_toStartOf="@+id/newHashtagName_et"
        app:layout_constraintStart_toStartOf="@+id/profile_guideline11"
        app:layout_constraintTop_toBottomOf="@+id/seekBar2" />

    <EditText
        android:id="@+id/newHashtagName_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:textStyle="bold"
        android:textSize="20sp"
        android:hint="내용을 입력하세요."
        app:layout_constraintBottom_toBottomOf="@+id/addHashtagNameButton"
        app:layout_constraintEnd_toStartOf="@+id/profile_guideline22"
        app:layout_constraintStart_toEndOf="@+id/addHashtagNameButton"
        app:layout_constraintTop_toTopOf="@+id/addHashtagNameButton" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/profile_guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/profile_guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />

    <Button
        android:id="@+id/hashtag_save_bt"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="30sp"
        android:textStyle="bold"
        android:text="선택완료"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/profile_guideline4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@color/button_light_intext_color"
        android:text="#추가된 해시태그"
        app:layout_constraintBottom_toTopOf="@+id/cs_hstag_bt1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/profile_guideline3" />

    <SeekBar
        android:id="@+id/seekBar2"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        android:max="20"
        android:progress="10"
        app:layout_constraintBottom_toTopOf="@+id/addHashtagNameButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/profile_guideline11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.10" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/profile_guideline22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.900" />

    <SeekBar
        android:id="@+id/seekBar1"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        android:max="20"
        android:progress="10"
        app:layout_constraintBottom_toTopOf="@+id/seekBar2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:textStyle="bold"
        android:textSize="20sp"
        android:text="활동적"
        app:layout_constraintBottom_toBottomOf="@+id/seekBar1"
        app:layout_constraintEnd_toStartOf="@+id/seekBar1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/seekBar1" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:textStyle="bold"
        android:textSize="20sp"
        android:text="정적"
        app:layout_constraintBottom_toBottomOf="@+id/seekBar1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/seekBar1"
        app:layout_constraintTop_toTopOf="@+id/seekBar1" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:textStyle="bold"
        android:textSize="20sp"
        android:text="좋음"
        app:layout_constraintBottom_toBottomOf="@+id/seekBar2"
        app:layout_constraintEnd_toStartOf="@+id/seekBar2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/seekBar2" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:textStyle="bold"
        android:textSize="20sp"
        android:text="나쁨"
        app:layout_constraintBottom_toBottomOf="@+id/seekBar2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/seekBar2"
        app:layout_constraintTop_toTopOf="@+id/seekBar2" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="210dp"
        android:layout_height="220dp"
        app:layout_constraintBottom_toTopOf="@+id/seekBar1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:src="@tools:sample/avatars" />

    <Button
        android:id="@+id/cs_hstag_bt1"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="-"
        app:layout_constraintBottom_toTopOf="@+id/profile_guideline4"
        app:layout_constraintEnd_toStartOf="@+id/cs_hstag_bt2"
        app:layout_constraintStart_toStartOf="@+id/profile_guideline11"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/cs_hstag_bt2"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="@+id/cs_hstag_bt1"
        app:layout_constraintEnd_toStartOf="@+id/cs_hstag_bt3"
        app:layout_constraintStart_toEndOf="@+id/cs_hstag_bt1"
        app:layout_constraintTop_toTopOf="@+id/cs_hstag_bt1" />

    <Button
        android:id="@+id/cs_hstag_bt3"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/common_rect_button_dark"
        android:fontFamily="@font/font1"
        android:textColor="@color/button_dark_intext_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="@+id/cs_hstag_bt2"
        app:layout_constraintEnd_toStartOf="@+id/profile_guideline22"
        app:layout_constraintStart_toEndOf="@+id/cs_hstag_bt2"
        app:layout_constraintTop_toTopOf="@+id/cs_hstag_bt2" />

    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/button_dark_color"
        app:layout_constraintBottom_toTopOf="@+id/profile_guideline1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/button_dark_color"
        app:layout_constraintBottom_toTopOf="@+id/profile_guideline2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/button_dark_color"
        app:layout_constraintBottom_toTopOf="@+id/profile_guideline3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/button_dark_color"
        app:layout_constraintBottom_toTopOf="@+id/profile_guideline4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>